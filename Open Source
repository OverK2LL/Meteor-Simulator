local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

MiscSection:NewToggle("Auto Claim Daily", "Auto Claims Daily", function(v)
    getgenv().autoswingers = v
    while true do
    if not getgenv().autoswingers then return end

    local A_1 = "ClaimPlaytimeReward"
    local A_2 = "1"
    local Event = game:GetService("ReplicatedStorage").Communication.RE
