if game.PlaceId == 9602700094 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Meteor Simulator", "BloodTheme")
 
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewToggle("Auto Rebirth", "Rebirths Automatically", function(v)
        getgenv().autoswinger = v
        while true do
        if not getgenv().autoswinger then return end
        local A_1 = "RebirthRequest"
        local Event = game:GetService("ReplicatedStorage").Communication.RE
        Event:FireServer(A_1)
    wait(3)        
        end
end)

    local Choices; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
    MainSection:NewDropdown("Select Target", "Select what you want to automatically destroy", {"City Stacks", "City Pile", "City Crate", "City Safe", "Desert Stacks", "Desert Pile", "Desert Crate", "Desert Safe", "Ocean Stacks", "Ocean Pile", "Ocean Crate", "Ocean Safe", "Winter Stacks", "Winter Pile", "Winter Crate", "Winter Safe", "Toxic Stacks", "Toxic Pile", "Toxic Crate", "Toxic Safe", "Candy Stacks", "Candy Pile", "Candy Crate", "Candy Safe", "Lava Stacks", "Lava Pile", "Lava Crate", "Lava Safe", "Electro Stacks", "Electro Pile", "Electro Crate", "Electro Safe", "Samurai Stacks", "Samurai Pile", "Samurai Crate", "Samurai Safe", "Heaven Stacks", "Heaven Pile", "Heaven Crate", "Heaven Safe",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
        Choices = value; --STORES THE USERS SELECTION
    end)


