local Teleport = Window:NewTab("Teleport")
local TeleportSection = Teleport:NewSection("Teleport")


local Tele; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
TeleportSection:NewDropdown("Select Destination", "Select where you wanna go Duh??", {"City", "Desert", "Atlantis", "Winter", "Toxic", "Candy", "Lava", "Electro", "Samurai", "Heaven", "Summer",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
    Tele = value; --STORES THE USERS SELECTION
end)

TeleportSection:NewButton("Teleport", "Teleports", function(v)

if Tele == "City" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, -119, 0, 0, 1, 0, 1, -0, -1, 0, 0)        
else if Tele == "Desert" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 146, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Atlantis" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 405, 0, 0, 1, 0, 1, -0, -1, 0, 0) 
else if Tele == "Toxic" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 654, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Candy" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 905, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Lava" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 1156, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Electro" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 1407, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Samurai" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 1664, 0, 0, 1, 0, 1, -0, -1, 0, 0)     
else if Tele == "Heaven" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 8.5, 1923, 0, 0, 1, 0, 1, -0, -1, 0, 0)                                     
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
end)



local Eggs = Window:NewTab("Eggs")
local EggsSection = Eggs:NewSection("Eggs")

local EC; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
EggsSection:NewDropdown("Select Egg", "Select What Egg You Want Duh??", {"City Egg", "Desert Egg", "Water Egg", "Winter Egg", "Toxic Egg", "Candy Egg", "Lava Egg", "Electro Egg", "Samurai Egg", "Heaven Egg",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
    EC = value; --STORES THE USERS SELECTION
end)

EggsSection:NewToggle("Auto Open", "Auto Opens Selected Egg ?/???/?", function(v)

    ---Loops---
getgenv().autoswings = v
while true do
if not getgenv().autoswings then return end
    ---Loops---

    if EC == "City Egg" then
        local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "City Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


    else if EC == "Desert Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Desert Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Water Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Water Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Winter Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Winter Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Toxic Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Toxic Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Candy Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Candy Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Lava Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Lava Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Electro Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Electro Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)


else if EC == "Samurai Egg Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "City Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
wait(0.5)

else if EC == "Heaven Egg" then
    local A_1 = "PetInv"
local A_2 = "HatchOnce"
local A_3 = "Heaven Egg"
local Event = game:GetService("ReplicatedStorage").Communication.RE
Event:FireServer(A_1, A_2, A_3)
